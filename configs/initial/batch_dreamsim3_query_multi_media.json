{
  "type": "batch",
  "template": "dreamsim3",
  "max_turns": 30,
  "models": ["sonnet3"],
  "max_context_frac": 0.0,
  "context_window": 128000,
  "integrations": {
    "discord": "narrative_terminal",
    "media": ["kieai", "cli"]
  },
  "data_source": {
    "kind": "supabase",
    "query": "rollercoaster",
    "limit": 50,
    "source": "all"
  },
  "template_vars_from_item": { "DREAM_TEXT": "content" },
  "output": { "meta_jsonl": "var/backrooms_logs/dreamsim3/dreamsim3_meta.jsonl" }
}

